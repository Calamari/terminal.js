{
  "globals": {
    "it": true,
    "sinon": true,
    "afterEach": true,
    "beforeEach": true,
    "after": true,
    "before": true,
    "describe": true,
    "expect": true
  },

// Enforcing
  "camelcase": true,
  "bitwise": true,
  "curly": true,
  "eqeqeq": true,
  "forin": false,
  "immed": true,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonew": false,
  "strict": false,
  "trailing": true,
  "undef": true,
  "unused": true,
  "maxparams": 4,
  "maxdepth": 999,
  "maxcomplexity": 999,
  "maxstatements": 999,
  "plusplus" : false,
  "quotmark" : "single",

// Relaxing
  "asi": false,
  "boss": true,
  "debug": false,
  "eqnull": true,
  "evil": false,
  "expr": true,
  "globalstrict": false,
  "laxbreak": false,
  "laxcomma": true,
  "loopfunc": false,
  "scripturl": true,
  "shadow": false,
  "sub": true,
  "supernew": false,

// Environments
  "browser": true,
  "couch": false,
  "devel": false,
  "jquery": true,
  "mootools": false,
  "node": true,
  "nonstandard": true,
  "prototypejs": false,
  "rhino": false,
  "wsh": true,

// Legacy
  "nomen": false,
  "onevar": true,
  "passfail": false,
  "white": false,

// ???
  "es5": false,
  "maxerr": 2000,
  "regexdash": false,
  "regexp": false
}
